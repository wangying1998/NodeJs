<!DOCTYPE html>
<html>
  <head>
    <title>Todo List</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Todo List</h1>
    <form action="/" method="POST">
      <input type="text" name='item'>
      <input type="submit" value="add">
    </form>
    <ul><%- items.map(function(item){return '    <li>'+item+'</li>';}).join('\n')%></ul>
  </body>
</html>
